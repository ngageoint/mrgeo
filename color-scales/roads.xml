<ColorMap name="Roads">
  <!-- Applies a color ramp to values in kph to differentiate roads. Style for OSM roads -->
  <Scaling>Absolute</Scaling>
  <ReliefShading>0</ReliefShading>
  <Interpolate>1</Interpolate>
  <NullColor color="0,0,0" opacity="0"/>
  <Color value="10" color="0,128,255"/>
  <Color value="20" color="92,174,162"/>
  <Color value="30" color="162,208,92"/>
  <Color value="40" color="208,231,46"/>
  <Color value="50" color="255,231,0"/>
  <Color value="60" color="255,208,0"/>
  <Color value="70" color="255,162,0"/>
  <Color value="80" color="255,112,0"/>
  <Color value="90" color="255,32,0"/>
  <Color value="100" color="255,0,0"/>
</ColorMap>
