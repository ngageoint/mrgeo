  <NullColor color="0,0,0" opacity="0"/>
  <!--  Dark Green  -->
  <Color value="0.0" color="45,89,0"/>
  <!--  Light Green  -->  
  <Color value="0.1" color="119,190,119"/>
  <!--  Yellow  -->
  <Color value="0.25" color="253,253,150"/>
  <!--  Orangish  -->
  <Color value="0.3" color="255,179,71"/>
  <!--  Brown  -->
  <Color value="0.55" color="130,105,83"/>
  <!--  White  -->
  <Color value="0.7" color="255,255,255"/>
</ColorMap>